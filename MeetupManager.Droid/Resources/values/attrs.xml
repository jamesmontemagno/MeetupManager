<?xml version="1.0" encoding="utf-8"?>
<resources>
  <!--Styleable custom imageview with height adjustable-->
  <declare-styleable name="MvxHeightAdjustableImageView">
    <attr name="ratio" format="reference|float"/><!--Ratio of width to height, deafult 1 so a .5 is height is half as hight as width-->
    <attr name="blur" format="boolean"/><!--if blur is enable, default is false-->
    <attr name="blur_radius" format="reference|float"/><!--Blur radius if blur is enabled default is 14-->
  </declare-styleable>

</resources>